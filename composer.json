{
    "name": "chazer/php-isp-base",
    "description": "This library can help you develop a PHP-based plugins for ISPsystem's software products",
    "type": "library",
    "keywords": ["ISPsystem", "plugin"],
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksandr Chazov",
            "email": "develop@chazer.ru"
        }
    ],
    "require": {
        "php":">=5.5.0",
        "symfony/console": "2.*",
        "symfony/http-foundation": "2.*",
        "symfony/process": "2.*",
        "psr/log": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "ISP\\": "src/ISP"
        },
        "files": ["src/functions.php"]
    }
}
